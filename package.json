{
  "name": "bidon",
  "version": "1.0.0",
  "description": "Fake repository for testing ci tools",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src",
    "docs",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:cjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:cjs && npm run build:es",
    "lint": "eslint .",
    "test": "npm run lint && echo OK || echo KO",
    "clean": "rimraf coverage dist lib es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumearm/bidon.git"
  },
  "keywords": [
    "bidon",
    "fake-repository",
    "dummy-repository"
  ],
  "author": "Guillaume ARM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guillaumearm/bidon/issues"
  },
  "homepage": "https://github.com/guillaumearm/bidon#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-es2015": "^7.0.0-beta.40",
    "@babel/preset-stage-0": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
